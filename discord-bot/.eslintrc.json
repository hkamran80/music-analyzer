{
    "extends": "eslint:recommended",
    "env": {
        "node": true,
        "es6": true
    },
    "parserOptions": {
        "ecmaVersion": 2021
    },
    "rules": {
        "semi": ["error", "always"],
        "prefer-const": "error",
        "no-var": "error",
        "no-prototype-builtins": "warn"
    }
}
